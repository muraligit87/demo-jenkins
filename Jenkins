pipeline {
    agent any
 
    stages {
        stage('Build') {
            steps {
                echo 'I am at build stage'
            }
        }
        stage('Test') {
            steps {
                echo 'I am at testing stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Example'
            }
        }
    }
}
